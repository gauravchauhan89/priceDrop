{
  "manifest_version": 2,

  "name": "Price Drops",
  "description": "This extension shows price drops in your selected products",
  "version": "0.1",
  "permissions": [
    "http://www.flipkart.com/",
    "http://code.jquery.com/"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  }
}